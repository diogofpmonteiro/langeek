  {{#each allPosts}}
  <div class="container">
    <div class="row">
      <div class="col-3">
      </div>
      <div class="col-6">
        <div class="card my-5 w-100" style="width: 25rem">
          {{!-- <h3> <img src="{{!-- post.profilepic --}}" alt="">{{!-- post.author --}}</h3> --}}
          <h5 class="card-title">{{this.title}}</h5>
          <img src="{{this.postPicture}}" class="card-img-top" alt="..." />
          <div class="card-body">
            <p class="card-text">
              {{this.description}}
            </p>
            
          <h5 class="card-title btn btn-primary">#{{this.languageTag}}
          </div>
        </div>
      </div>
      <div class="col-3">
      </div>
    </div>
  </div>
{{/each}}
  